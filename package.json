{
  "name": "@jurono/sdk",
  "version": "1.1.0",
  "description": "TypeScript SDK for Jurono API",
  "type": "module",
  "engines": {
    "node": ">=20.11.1"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.esm.js",
      "require": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./nextjs": {
      "import": "./dist/nextjs.esm.js",
      "require": "./dist/nextjs.js",
      "types": "./dist/nextjs.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "typecheck": "tsc --noEmit --skipLibCheck --project tsconfig.build.json",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "generate:types": "npx openapi-typescript https://api.jurono.eu/api/docs-json -o src/types/api.d.ts",
    "prepublishOnly": "npm run generate:types && npm run build",
    "release": "release-it"
  },
  "keywords": [
    "jurono",
    "api",
    "sdk",
    "typescript",
    "legal",
    "react",
    "nextjs",
    "ssr",
    "ssg"
  ],
  "author": "Jurono",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/jest": "30.0.0",
    "@types/node": "24.3.1",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "eslint": "9.35.0",
    "jest": "30.1.3",
    "openapi-typescript": "^7.9.1",
    "react": "^18.0.0",
    "release-it": "^17.6.0",
    "rollup": "^4.50.1",
    "rollup-plugin-dts": "^6.2.3",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.8.0",
    "@types/react": ">=16.8.0",
    "next": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurono/jurono-sdk-ts.git"
  },
  "bugs": {
    "url": "https://github.com/jurono/jurono-sdk-ts/issues"
  },
  "homepage": "https://github.com/jurono/jurono-sdk-ts#readme"
}

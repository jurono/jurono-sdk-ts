{
  "name": "@jurono/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Jurono API",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "generate:types": "npx openapi-typescript https://api.jurono.eu/api/docs-json -o src/types/api.d.ts",
    "prepublishOnly": "npm run generate:types && npm run build",
    "publish": "npm publish --access public"
  },
  "release": {
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "keywords": [
    "jurono",
    "api",
    "sdk",
    "typescript",
    "legal"
  ],
  "author": "Jurono",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/jest": "30.0.0",
    "@types/node": "24.3.1",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "eslint": "9.35.0",
    "jest": "30.1.3",
    "openapi-typescript": "^7.9.1",
    "rollup": "^4.50.1",
    "rollup-plugin-dts": "^6.2.3",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jurono/sdk-js-api.git"
  },
  "bugs": {
    "url": "https://github.com/jurono/sdk-js-api/issues"
  },
  "homepage": "https://github.com/jurono/sdk-js-api#readme"
}
